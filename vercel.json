{
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "rewrites": [
    
    {
      "source": "/",
      "destination": "/"
    },
    {
      "source": "/vps_panel",
      "destination": "/vps_panel"
    },
    
    {
      "source": "/login",
      "destination": "/server"
    },
    {
      "source": "/register",
      "destination": "/server"
    },
    {
      "source": "/logout",
      "destination": "/server"
    },
    
    {
      "source": "/shell",
      "destination": "/server"
    },
    {
      "source": "/files",
      "destination": "/server"
    },
    {
      "source": "/view-archive",
      "destination": "/server"
    },
    {
      "source": "/loadfile",
      "destination": "/server"
    },
    
    {
      "source": "/upload",
      "destination": "/server"
    },
    {
      "source": "/delete-files",
      "destination": "/server"
    },
    {
      "source": "/archive",
      "destination": "/server"
    },
    {
      "source": "/extract",
      "destination": "/server"
    },
    {
      "source": "/savefile",
      "destination": "/server"
    }
  ]
}
