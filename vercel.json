{
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "rewrites": [
    // Rute Halaman Next.js (Dibiarkan Next.js yang menangani GET)
    {
      "source": "/",
      "destination": "/"
    },
    {
      "source": "/vps_panel",
      "destination": "/vps_panel"
    },
    
    
    {
      "source": "/login",
      "destination": "/server"
    },
    {
      "source": "/register",
      "destination": "/server"
    },
    {
      "source": "/logout",
      "destination": "/server"
    },
    
    {
      "source": "/shell",
      "destination": "/server"
    },
    {
      "source": "/files",
      "destination": "/server"
    },
    {
      "source": "/upload",
      "destination": "/server"
    },
    
    {
      "source": "/:path(delete-files|archive|extract|view-archive|loadfile|savefile)",
      "destination": "/server"
    }
  ]
}
