{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/"
    },
    {
      "source": "/vps_panel",
      "destination": "/vps_panel"
    },
    {
      "source": "/login",
      "destination": "/login"
    },
    {
      "source": "/register",
      "destination": "/register"
    },
    {
      "source": "/logout",
      "destination": "/logout"
    },
    
    {
      "source": "/:path(shell|files|view-archive|loadfile|upload|delete-files|archive|extract|savefile)",
      "destination": "/server"
    },
    
    {
      "source": "/login",
      "destination": "/server"
    },
    {
      "source": "/register",
      "destination": "/server"
    },
    {
      "source": "/logout",
      "destination": "/server"
    }
  ]
}
